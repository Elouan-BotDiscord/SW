{
  "version": 2,
  "builds": [
    {
      "src": "Siege/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "Siege/server.js"
    },
    {
      "src": "/app.js",
      "dest": "Siege/app.js"
    },
    {
      "src": "/mapping.js",
      "dest": "Siege/mapping.js"
    },
    {
      "src": "/(.*)",
      "dest": "Siege/server.js"
    }
  ]
}
